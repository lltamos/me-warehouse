<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mesite.infrastructure.gatewayimpl.database.mapper.ITmsTestPaperShipMapper">
	<select id="findTmsPaperIdOrderByRank" resultType="com.mesite.infrastructure.gatewayimpl.database.dataobject.TmsTestPaperShip">
		select * from tms_test_paper_ship
		<where>
			<if test="tmsTestPaperId!=null">
				tms_test_reps_id=#{tmsTestPaperId}
			</if>
		</where>
		order by utime
	</select>
</mapper>
